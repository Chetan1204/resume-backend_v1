<div style="display: flex; align-items: center; justify-content: center;position: fixed;top: 0;left: 0; right: 0;bottom: 0;z-index: 100;background-color: aliceblue;">
	<div style="width: 30%; background-color: #fff;display: flex;flex-direction: column;align-items: start;justify-content: start;padding: 15px;">
		<div style="width: fit-content;margin: 0 auto;display: flex;align-items: center;padding: 15px 0px;">
			<img src="/static/icons8-secure-50.png" style="margin-right: 10px;" width="50" alt="Login">
			<h3 style="margin: 0;font-weight: 600;">LOG IN</h3>
		</div>
		<form style="width: 100%;" action="/api/v1/manage/auth/admin-login" method="POST">
			<div style="width: 100%;">
				<label class="form-label mt-3" for="email">
				EMAIL
			</label>
			<input class="form-control mb-3"
			type="text"
			id="email"
			name="email" value="" placeholder="xzy@example.com">
			</div>
			<div style="width: 100%;">
				<label class="form-label mt-3" for="password">
					PASSWORD
				</label>
				<input class="form-control"
				type="password"
				id="password"
				name="password" value="">
				<span style="display: block;font-size: 12px;margin-top: 5px;color: #787878;" class="mb-3">Passwords are end to end encrypted</span>
			</div>
			<button type="submit" class="button-main button-main-md" style="width: 100%;padding: 10px;">Login</button>
		</form>
	</div>
</div>