<div style="width: 100%;height: 100%;padding: 15px;">
	<div>
		<h3>All Models</h3>
		<p>Create, edit, and manage the models of your site.</p>
	</div>
	<table class="table table-striped">
		<thead style="border-bottom: 4px solid #315BC1;">
		  <tr>
			<th>
				<div style="display: flex; align-items: center; justify-content: start;">
					<h3 style="font-weight: 600;font-size: 16px;">Models</h3>  
				</div>
			</th>
			<th>
				<div style="display: flex; align-items: center; justify-content: end;">
					<span style="background-color: #315BC1;color: #fff;padding: 7px 16px;font-size: 13x;font-weight: 600;cursor: pointer;" onclick="handleAddNewModel()">Add New Model</span>
				</div>
			</th>
			
		  </tr>
		</thead>
		<tbody>
			<% allModels.forEach(model => { %>
				<tr class="page-tr">
					<td style="width: 80%;">
						<span style="font-weight: 400;"><%= model.modelName %></span>
						<span style="display: block; width: 100%;">
							<span style="cursor: pointer;color: green;font-size: 14px;padding: 0px; font-weight: 600;" onclick="editPage('<%= model.modelName %>')">Edit</span>
						</span>
					</td>
				</tr>
	
			<% }) %>
		</tbody>
	  </table>

</div>




<script>
function handleAddNewModel() {
    window.location.href = "/manage/add-new-model"
} 
function editPage(modelname){
    window.location.href = `/manage/rendermodel?modelname=${encodeURIComponent(modelname)}`

}
</script>